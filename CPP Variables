Variables

#include <iostream>

using namespace std;

int main()
{
	
    string characterName = "John" ;
    int characterAge;
    characterAge = 50 ;
    
      cout << "There was once a man named " << characterName << endl;
      cout << "He was " << characterAge << " years old" << endl;
    
    characterName = "Lisa" ; characterAge = 30 ;
    
      cout << "He liked his name " << characterName << endl;
      cout << "But he didn't like being " << characterAge << endl;
    
    return 0;
    
}
